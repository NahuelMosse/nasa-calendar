{
  "plugins": ["unused-imports"],
  "extends": "next/core-web-vitals",
  "rules": {
    "eqeqeq": "error",
    "quotes": ["error", "double"],
    "semi": ["error", "always"],
    "indent": ["error", 4],
    "no-var": "error",
    "prefer-const": "error",
    "no-console": "warn",
    "no-unused-vars": "off",
		"unused-imports/no-unused-imports": "error",
		"unused-imports/no-unused-vars": [
			"error",
			{ "vars": "all", "varsIgnorePattern": "^_", "args": "after-used", "argsIgnorePattern": "^_" }
		]
  }
}
